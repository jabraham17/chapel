#!/bin/sh 

TESTNAME=$1
OUTFILE=$2
TMPFILE=$OUTFILE.prediff.tmp

# filter out line numbers
sed -E 's/\.chpl:[0-9]*:/\.chpl:n:/' < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

# filter out record/class/union
sed -E 's/class|record|union/AT/' < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

# filter out sync/single
sed -E 's/sync|single/SYNCSINGLE/' < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

rm $TMPFILE
